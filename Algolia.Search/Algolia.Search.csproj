<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <Description>Algolia Search API Client for C#</Description>
        <AssemblyTitle>Algolia.Search</AssemblyTitle>
        <VersionPrefix>2.0.2</VersionPrefix>
